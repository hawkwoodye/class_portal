<h1>Students#index</h1>
<p>Welcome to Student Self-Center!</p>

<%= " #{session[:my_role]} " %>

<h2>Log in</h2>

<%= "Welcome to #{session[:my_role]} Page! " %>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag :action => 'login' do  %>

      <%= label_tag :email %>
      <%= email_field 'student', 'email' %>

      <%= label_tag :encrypted_password %>
      <%= password_field 'student','encrypted_password' %>

      <%= submit_tag "Log in", class: "btn btn-primary" %>
    <% end %>

    <p>New user? <%= link_to "Sign up now!", '/students/new' %></p>
  </div>
</div>



